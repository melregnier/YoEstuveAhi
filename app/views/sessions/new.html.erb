<div class="container p-3 my-3 border">
   <h1>Completá tus credenciales</h1>
</div>
<div class="container p-3 my-3 border">
   <div class="form-group">
      <%= form_tag '/login' do %>
      <%= label_tag :Email%><br>
      <%= email_field_tag :email%><br>
   </div>
   <div class="form-group">
      <%= label_tag :Contraseña%><br>
      <%= password_field_tag :password%><br>
   </div>
   <%= submit_tag "Ingresar", class: "btn btn-success"%>
   <%end%>
</div>
